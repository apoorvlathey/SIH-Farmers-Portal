{
    "extends": ["wesbos"],
    "rules": {
      "react/prop-types": 0,
      "no-console": 0,
      "react-hooks/rules-of-hooks": 2,
      "react-hooks/exhaustive-deps": 1,
      "import/no-extraneous-dependencies": 0,
      "global-require": 0
    },
    "plugins": ["react", "import", "jsx-a11y", "react-hooks"],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    }
  }